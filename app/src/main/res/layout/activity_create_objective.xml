<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <View
        android:id="@+id/view"
        android:layout_width="0dp"
        android:layout_height="72dp"
        android:background="@color/blue_main"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/image_back_object"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="6dp"
        android:src="@drawable/close_x"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_new_object"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="@string/new_objective"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/view"
        app:layout_constraintStart_toEndOf="@id/image_back_object"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_name_object"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:text="@string/name"
        android:textColor="@color/gray_second"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view"

        />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edittext_name_meta"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="8dp"
        android:hint="@string/name_meta"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:padding="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_name_object" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_value_object"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp"
        android:text="@string/value_objective"
        android:textColor="@color/gray_second"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edittext_name_meta"

        />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edittext_value_object"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="8dp"
        android:hint="@string/value_objective"
        android:inputType="numberDecimal"
        android:minHeight="48dp"
        android:padding="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_value_object" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_already_saved"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp"
        android:text="@string/already_saved"
        android:textColor="@color/gray_second"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edittext_value_object" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edittext_already_saved"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="8dp"
        android:hint="@string/already_saved"
        android:inputType="numberDecimal"
        android:minHeight="48dp"
        android:padding="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_already_saved" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_desired_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp"
        android:text="@string/desired_date"
        android:textColor="@color/gray_second"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edittext_already_saved" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edittext_desired_date"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="8dp"
        android:hint="@string/desired_date"
        android:inputType="date"
        android:minHeight="48dp"
        android:padding="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_desired_date" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp"
        android:text="@string/icon"
        android:textColor="@color/gray_second"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edittext_desired_date"

        />


    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/image_health"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="18dp"
        android:src="@drawable/ob__1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_icon" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_stores"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="50dp"
        android:src="@drawable/ob__2"
        app:layout_constraintBottom_toBottomOf="@+id/image_health"
        app:layout_constraintStart_toEndOf="@id/image_health"
        app:layout_constraintTop_toTopOf="@id/image_health" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_phone"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="50dp"
        android:src="@drawable/ob__3"
        app:layout_constraintBottom_toBottomOf="@id/img_stores"
        app:layout_constraintStart_toEndOf="@+id/img_stores"
        app:layout_constraintTop_toTopOf="@+id/img_stores" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_shopping"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="18dp"
        android:src="@drawable/ob__4"
        app:layout_constraintBottom_toBottomOf="@id/img_phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_phone"
        app:layout_constraintTop_toTopOf="@id/img_phone" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_present"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/ob__5"
        app:layout_constraintStart_toStartOf="@+id/image_health"
        app:layout_constraintTop_toBottomOf="@+id/image_health" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_change"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/ob__6"
        app:layout_constraintStart_toStartOf="@+id/img_stores"
        app:layout_constraintTop_toBottomOf="@+id/img_stores" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_travel"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/ob__7"
        app:layout_constraintStart_toStartOf="@+id/img_phone"
        app:layout_constraintTop_toBottomOf="@+id/img_phone" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_email"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/ob__8"
        app:layout_constraintStart_toStartOf="@+id/img_shopping"
        app:layout_constraintTop_toBottomOf="@+id/img_shopping" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="50dp"
        android:background="@drawable/background_button"
        android:text="@string/Create"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
